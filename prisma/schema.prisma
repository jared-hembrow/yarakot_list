// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Vegetables {
  id String @id @default(cuid())
  englishName String
  hebrewName String
  url String
  amount Int @default(0)
  containerType String @default("box")
  order Int
}
model Orders {
  id String @id @default(cuid())
  list String
  timestamp Int

}
model User {
  id String @id @default(cuid())
  name String
  password String
}

